{
   "compilerOptions": {
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "declaration": true,
      "allowJs": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "suppressImplicitAnyIndexErrors": true,
      "strictNullChecks": false,
      "noUnusedLocals": true,
      "noImplicitThis": true,
      "noUnusedParameters": true,
      "importHelpers": true,
      "noEmitHelpers": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "target": "es2015",
      "jsx": "react",
      "experimentalDecorators": false,
      "allowSyntheticDefaultImports": true,
      "lib": ["es6", "dom"]
   },
   "formatCodeOptions": {
      "indentSize": 2,
      "tabSize": 2
   }
}
